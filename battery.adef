version: 1.0
sandboxed: false
start: auto

executables:
{
    battery = ( batteryComponent )
}

processes:
{
    envVars:
    {
        LE_LOG_LEVEL = INFO
    }
    run:
    {
        ( battery )
    }
    faultAction: stopApp
}
requires:
{
    configTree:
    {
        [rw] .
    }
}

extern:
{
    battery.batteryComponent.ma_battery
    battery.batteryComponent.ma_adminbattery
}

bindings:
{
    battery.batteryComponent.dhubIO -> dataHub.io
}
